{
   "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "baseUrl": "./",
      "paths": {
         "*": [
            "node_modules/*",
            "src/*"
         ]
      }
   },
   "include": [
      "src/**/*"
   ],
   "exclude": [
      "node_modules",
      "dist"
   ]
}